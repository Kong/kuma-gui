{
  "name": "kuma-gui",
  "version": "0.1.0",
  "private": true,
  "description": "Kuma Manager",
  "author": "Kong",
  "scripts": {
    "serve": "yarn vue-cli-service serve",
    "build": "yarn vue-cli-service build --mode production",
    "lint": "yarn vue-cli-service lint",
    "test:e2e": "yarn vue-cli-service test:e2e",
    "test:unit": "yarn vue-cli-service test:unit"
  },
  "dependencies": {
    "@appscode/json2yaml": "^0.1.2",
    "@kongponents/kalert": "0.1.3",
    "@kongponents/kbadge": "^0.1.2",
    "@kongponents/kbutton": "0.1.10",
    "@kongponents/kcard": "0.1.3-1",
    "@kongponents/kclipboardprovider": "0.1.2",
    "@kongponents/kemptystate": "0.0.1-beta.4",
    "@kongponents/kicon": "0.3.0",
    "@kongponents/kinput": "^0.2.0-0",
    "@kongponents/kmodal": "^0.1.4-8",
    "@kongponents/kooltip": "0.2.0",
    "@kongponents/kpop": "0.2.10",
    "@kongponents/krumbs": "0.0.1",
    "@kongponents/ktable": "0.1.4-1",
    "@kongponents/ktoaster": "0.0.1-beta.4",
    "@kongponents/ktoggle": "^0.1.1",
    "@kongponents/styles": "0.0.4",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "tailwindcss": "^1.1.2",
    "vue": "^2.6.10",
    "vue-meta": "^2.3.1",
    "vue-prismjs": "^1.2.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-e2e-cypress": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "raw-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "standard": "^14.3.1",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0",
    "yarn": "^1.13.0"
  }
}
