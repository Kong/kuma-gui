{
  "name": "kuma-gui",
  "version": "0.1.0",
  "private": true,
  "description": "Kuma Manager",
  "author": "Kong",
  "scripts": {
    "serve": "yarn vue-cli-service serve",
    "prebuild": "yarn msw:cleanup",
    "build": "yarn vue-cli-service build --mode production",
    "build:ci": "yarn vue-cli-service build --mode development",
    "test:unit": "yarn vue-cli-service test:unit -i",
    "lint": "yarn vue-cli-service lint",
    "msw:cleanup": "rm -f public/mockServiceWorker.js",
    "test:unit:watch": "vue-cli-service test:unit --watch -i"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.26",
    "@appscode/json2yaml": "^0.1.2",
    "@datadog/browser-logs": "^4.17.2",
    "@kongponents/kalert": "^6.40.0",
    "@kongponents/kbadge": "^6.40.0",
    "@kongponents/kbutton": "^6.40.0",
    "@kongponents/kcard": "^6.40.0",
    "@kongponents/kclipboardprovider": "^6.40.0",
    "@kongponents/kemptystate": "^6.40.0",
    "@kongponents/kicon": "^6.40.0",
    "@kongponents/kmodal": "^6.40.0",
    "@kongponents/kpop": "^6.40.0",
    "@kongponents/kradio": "^6.40.0",
    "@kongponents/krumbs": "^6.40.0",
    "@kongponents/ktablelegacy": "^6.40.0",
    "@kongponents/ktabs": "^6.40.0",
    "@kongponents/styles": "^6.40.0",
    "axios": "^0.27.2",
    "core-js": "^3.24.1",
    "lodash": "^4.17.21",
    "semver": "^7.3.7",
    "semver-compare": "^1.0.0",
    "tailwindcss": "^3.1.8",
    "vue": "^2.7.8",
    "vue-meta": "^2.4.0",
    "vue-prismjs": "^1.2.0",
    "vue-router": "^3.5.4",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^5.8.3",
    "@types/jest": "^28.1.7",
    "@types/lodash": "^4.14.183",
    "@types/semver": "^7.3.12",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^28.1.0",
    "assert": "^2.0.0",
    "babel-jest": "^28.1.3",
    "babel-runtime": "^6.26.0",
    "browserify-zlib": "^0.2.0",
    "eslint": "^8.22.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "https-browserify": "^1.0.0",
    "msw": "^0.44.2",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "sass": "^1.54.4",
    "sass-loader": "^13.0.2",
    "standard": "^17.0.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12",
    "ts-jest": "^28.0.8",
    "tty-browserify": "^0.0.1",
    "typescript": "^4.7.4",
    "url": "^0.11.0",
    "vue-template-compiler": "^2.7.8",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts"
    ],
    "transform": {
      "^.+\\.vue$": "@vue/vue2-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!(@amcharts)\\/).+\\.js$"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^.+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/src/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest-setup.ts"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0",
    "yarn": "^1.22.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
