{
  "name": "kuma-gui",
  "version": "0.1.0",
  "private": true,
  "description": "Kuma Manager",
  "author": "Kong",
  "main": "src/index.ts",
  "scripts": {
    "dev": "yarn run dev:mock-api",
    "dev:mock-api": "cross-env VITE_MOCK_API_ENABLED=true vite -c ./vite.config.development.ts",
    "dev:real-api": "vite -c ./vite.config.development.ts",
    "preview": "vite preview -c ./vite.config.development.ts",
    "build": "vite build -c ./vite.config.ts",
    "build:ci": "cross-env VITE_MOCK_API_ENABLED=true VITE_FAKE_MULTIZONE=true yarn run build --mode preview -c ./vite.config.development.ts",
    "build:preview": "yarn run build:ci && mkdir gui && mv dist/* gui && mv gui dist/gui && mv dist/gui/mockServiceWorker.js dist/ && cp _redirects dist/_redirects",
    "lint": "eslint --ext .js,.ts,.vue --fix .",
    "lint:lockfile": "lockfile-lint --path yarn.lock --allowed-hosts yarn --validate-https",
    "lint:ts": "vue-tsc --noEmit",
    "test": "cross-env TZ=UTC jest",
    "test:watch": "yarn run test --watch"
  },
  "engines": {
    "node": ">=18.13",
    "yarn": "^1.22.0"
  },
  "dependencies": {
    "@datadog/browser-logs": "^4.36.0",
    "@kong/kongponents": "^8.41.3",
    "brandi": "^5.0.0",
    "chart.js": "^4.2.1",
    "js-yaml": "^4.1.0",
    "prismjs": "^1.29.0",
    "tailwindcss": "^3.2.7",
    "vue": "^3.2.47",
    "vue-chartjs": "^5.2.0",
    "vue-github-button": "^3.1.0",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.0",
    "@faker-js/faker": "^7.6.0",
    "@jest/globals": "^29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/prismjs": "^1.26.0",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.7",
    "@vue/vue3-jest": "^29.2.3",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "canvas": "^2.11.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.10.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-serializer-vue": "^3.1.0",
    "jest-transform-stub": "^2.0.0",
    "lockfile-lint": "^4.10.1",
    "msw": "~1.2.1",
    "postcss": "^8.4.21",
    "sass": "^1.60.0",
    "standard": "^17.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "msw": {
    "workerDirectory": "public"
  }
}
