{
  "name": "kuma-gui",
  "version": "2.2.0",
  "private": true,
  "description": "Kuma Manager",
  "author": "Kong",
  "main": "src/index.ts",
  "scripts": {
    "deps": "cypress install",
    "dev": "yarn run dev:mock-api",
    "dev:mock-api": "vite -c ./vite.config.development.ts",
    "dev:real-api": "cross-env VITE_MOCK_API_ENABLED=false yarn dev",
    "preview": "yarn build:staging && yarn preview:built",
    "preview:built": "vite preview -c ./vite.config.preview.ts",
    "build": "vite build -c ./vite.config.production.ts",
    "build:ci": "yarn run build --mode preview -c ./vite.config.development.ts",
    "build:gui": "mkdir gui && mv dist/* gui && mv gui dist/gui && mv dist/gui/mockServiceWorker.js dist/",
    "build:preview": "yarn run build:ci && yarn build:gui && cp _redirects dist/_redirects",
    "build:staging": "cross-env VITE_ZONE_CREATION_FLOW=enabled yarn build && yarn build:gui",
    "lint": "eslint --ext .js,.ts,.vue --fix .",
    "lint:lockfile": "lockfile-lint --path yarn.lock --allowed-hosts yarn --validate-https",
    "lint:ts": "vue-tsc --noEmit",
    "lint:styles": "stylelint --allow-empty-input './src/**/*.{css,scss,vue}'",
    "test": "cross-env TZ=UTC VITE_ZONE_CREATION_FLOW=enabled jest",
    "test:browser": "cross-env TZ=UTC cypress run --browser chrome",
    "test:browser:view": "cross-env TZ=UTC KUMA_BASE_URL=http://localhost:8080/gui cypress open --e2e --browser chrome",
    "test:watch": "yarn run test --watch",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview"
  },
  "engines": {
    "node": ">=18.17",
    "yarn": "^1.22.0"
  },
  "dependencies": {
    "@kong-ui-public/i18n": "^0.8.2",
    "@kong/icons": "^1.5.0",
    "@kong/kongponents": "^8.123.7",
    "brandi": "^5.0.0",
    "chart.js": "^4.4.0",
    "deepmerge": "^4.3.1",
    "js-yaml": "^4.1.0",
    "prismjs": "^1.29.0",
    "semver": "^7.5.4",
    "vue": "^3.3.4",
    "vue-chartjs": "^5.2.0",
    "vue-github-button": "^3.1.0",
    "vue-router": "^4.2.4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.22.15",
    "@badeball/cypress-cucumber-preprocessor": "^18.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@faker-js/faker": "^8.1.0",
    "@jest/globals": "^29.6.4",
    "@kong/design-tokens": "^1.10.2",
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "@types/js-yaml": "^4.0.6",
    "@types/marked": "^5.0.1",
    "@types/prismjs": "^1.26.0",
    "@types/semver": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/vue3-jest": "^29.2.6",
    "autoprefixer": "^10.4.15",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "canvas": "^2.11.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.1.0",
    "cypress-fail-fast": "^7.0.3",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.49.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "glob": "^10.3.4",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-serializer-vue": "^3.1.0",
    "jest-transform-stub": "^2.0.0",
    "lockfile-lint": "^4.12.1",
    "marked": "^9.0.3",
    "msw": "~1.3.0",
    "postcss-html": "^1.5.0",
    "sass": "^1.67.0",
    "standard": "^17.1.0",
    "stylelint": "^15.10.3",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.2.2",
    "urlpattern-polyfill": "^9.0.0",
    "vite": "^4.4.9",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vite-svg-loader": "^4.0.0",
    "vitepress": "^1.0.0-rc.14",
    "vue-tsc": "^1.8.10",
    "yaml-jest": "^1.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "msw": {
    "workerDirectory": "public"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "./cypress/main.ts",
      "./cypress/support/step_definitions/*.ts"
    ]
  }
}
