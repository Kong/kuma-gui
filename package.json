{
  "name": "kuma-gui",
  "version": "0.1.0",
  "private": true,
  "description": "Kuma Manager",
  "author": "Kong",
  "scripts": {
    "serve": "VUE_APP_MOCK_API_ENABLED=true yarn vue-cli-service serve",
    "serve:with-api": "yarn vue-cli-service serve",
    "build": "yarn vue-cli-service build",
    "build:ci": "VUE_APP_MOCK_API_ENABLED=true yarn run build --mode development",
    "lint": "yarn vue-cli-service lint",
    "test:unit": "yarn vue-cli-service test:unit",
    "test:unit:ci": "yarn run test:unit --runInBand",
    "test:unit:watch": "yarn run test:unit --watch"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.27",
    "@appscode/json2yaml": "^0.1.2",
    "@datadog/browser-logs": "^4.17.2",
    "@kong/kongponents": "7.0.0-beta.87",
    "core-js": "^3.24.1",
    "lodash": "^4.17.21",
    "prismjs": "^1.29.0",
    "semver": "^7.3.7",
    "semver-compare": "^1.0.0",
    "tailwindcss": "^3.1.8",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.184",
    "@types/prismjs": "^1.26.0",
    "@types/semver": "^7.3.12",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/vue3-jest": "^28.1.0",
    "assert": "^2.0.0",
    "babel-jest": "^28.1.3",
    "babel-plugin-prismjs": "^2.1.0",
    "babel-runtime": "^6.26.0",
    "browserify-zlib": "^0.2.0",
    "eslint": "^8.22.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.4.0",
    "https-browserify": "^1.0.0",
    "isomorphic-fetch": "^3.0.0",
    "msw": "^0.45.0",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "standard": "^17.0.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12",
    "ts-jest": "^28.0.8",
    "tty-browserify": "^0.0.1",
    "typescript": "~4.7.4",
    "url": "^0.11.0",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts"
    ],
    "transform": {
      "^.+\\.vue$": "@vue/vue3-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)\\?.+$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!(@amcharts)\\/).+\\.js$"
    ],
    "moduleNameMapper": {
      "^@/(.*(svg|png|gif))(\\?(inline|external))$": "<rootDir>/src/$1",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^.+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)\\?.+$": "jest-transform-stub"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/src/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest-setup.ts"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0",
    "yarn": "^1.22.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
