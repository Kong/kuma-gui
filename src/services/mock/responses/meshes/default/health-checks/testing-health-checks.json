{
  "type":"HealthCheck",
  "mesh":"default",
  "name":"testing-health-checks",
  "sources":[
    {
      "match":{
        "service":"web"
      }
    }
  ],
  "destinations":[
    {
      "match":{
        "service":"backend"
      }
    }
  ],
  "conf":{
    "activeChecks":{
      "interval":"10s",
      "timeout":"2s",
      "unhealthyThreshold":3,
      "healthyThreshold":4
    }
  }
}
