{
  "type": "HealthCheck",
  "mesh": "default",
  "name": "web-to-banana",
  "sources": [
    {
      "match": {
        "service": "web"
      }
    }
  ],
  "destinations": [
    {
      "match": {
        "service": "backend"
      }
    }
  ],
  "conf": {
    "activeChecks": {
      "interval": "10s",
      "timeout": "2s",
      "unhealthyThreshold": 123,
      "healthyThreshold": 12
    }
  }
}
